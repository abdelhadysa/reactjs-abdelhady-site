{
  "name": "reactjs-abdelhady-site",
  "version": "1.0.0",
  "description": "Abdelhady's Personal Site",
  "main": "./src/server/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "webpack-dev-server --config webpack.dev.js --open",
    "build-prod": "webpack --config webpack.prod.js",
    "start": "nodemon ./dist/index.js",
    "create-db": "npx sequelize-cli db:create",
    "reset-db": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "gen-model": "npx sequelize-cli model:generate",
    "migration": "npx sequelize-cli db:migrate",
    "undo-migration": "npx sequelize-cli db:migrate:undo",
    "undo-all-migrations": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "gen-seed": "npx sequelize-cli seed:generate",
    "undo-seed": "npx sequelize-cli db:seed:undo",
    "undo-all-seeds": "npx sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h2o-creator/reactjs-abdelhady-site.git"
  },
  "keywords": [
    "abdelhady",
    "salah",
    "react",
    "js",
    "personal",
    "website"
  ],
  "author": "Abdelhady \"H2O\" Salah",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/h2o-creator/reactjs-abdelhady-site/issues"
  },
  "homepage": "https://github.com/h2o-creator/reactjs-abdelhady-site#readme",

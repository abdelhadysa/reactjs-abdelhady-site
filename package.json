    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-device": "^0.4.2",
    "express-rate-limit": "^6.3.0",
    "helmet": "^5.0.2",
    "i18next": "^21.6.14",
    "i18next-browser-languagedetector": "^6.1.3",
    "i18next-http-backend": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.3.1",
    "morgan": "^1.10.0",
